
class Int => {
    Int(this.value);
    value: @int;
    
    operators => {
        add(other: Int) = Int(value + other.value);
        add(other: Float) = Float(value + other.value);

        sub(other: Int) = Int(value + other.value);
        sub(other: Float) = Float(value + other.value);
    };
};
