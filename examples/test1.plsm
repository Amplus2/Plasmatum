define abc(x y z) = x * y + z

define add(x y) = x + y
define sub(x y) = x - y
define double(x) = 2 * x

define fib(x) =
    if x <= 1
        x
    else
        fib(x - 1) + fib(x - 2)

define main() => (
    println('This is unicode! Σίγμα')
    println(double(21) add(40 2) sub(fib(10) 13) abc(2 3 18))
    return 42
)
